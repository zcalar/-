a=50
b=1.83
bmi=a/(b**2)
c=bmi
print('你的BIM指数为：%0.1f' % bmi)
if c<18.5:
    print('你的正常体重为：','%0.1f'%(18.5*(b**2)),"~",'%0.1f'%(25*(b**2)))
elif c>25:
    print('你的正常体重为：','%0.1f'%(18.5*(b**2)),"~",'%0.1f'%(25*(b**2)))
if bmi<18.5:
    print('过轻')
elif (bmi>=18.5)&(bmi<25):
    print('正常')
elif (bmi >=25)&(bmi<28):
    print('过重')
elif (bmi>=28)&(bmi<32):
    print('肥胖')
elif (bmi>=32)&(bmi<40):
    print('严重肥胖')
else:
    pass
